<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="res_partner_form_view_inherit_company_branch" model="ir.ui.view">
        <field name="name">res.partner.form.view.inherit.company_branch</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page string="HR" name="branch_office">
                    <group>
                        <field name="branch_office"/>
                    </group>
                </page>
            </xpath>
        </field>
    </record>

</odoo>
