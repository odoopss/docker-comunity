<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="section_lbs_tree_view" model="ir.ui.view">
        <field name="name">section.lbs.tree.view</field>
        <field name="model">section.lbs</field>
        <field name="arch" type="xml">
            <tree>
                <field name="code"/>
                <field name="description"/>
            </tree>
        </field>
    </record>

    <record id="section_lbs_form_view" model="ir.ui.view">
        <field name="name">section.lbs.form.view</field>
        <field name="model">section.lbs</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="code" readonly="1"/>
                        <field name="description"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

</odoo>
